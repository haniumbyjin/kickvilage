<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<typeAlias type="com.eunhasu.kickvillage.model.User" alias="User" />
		<typeAlias type="com.eunhasu.kickvillage.model.Admin" alias="Admin" />
		<typeAlias type="com.eunhasu.kickvillage.model.Station" alias="Station" />
		<typeAlias type="com.eunhasu.kickvillage.model.Reserve" alias="Reserve" />
		<typeAlias type="com.eunhasu.kickvillage.model.Kickboard" alias="Kickboard" />
		<typeAlias type="com.eunhasu.kickvillage.model.Board" alias="Board" />
		<typeAlias type="com.eunhasu.kickvillage.model.PageMaker" alias="PageMaker" />
		<typeAlias type="com.eunhasu.kickvillage.model.Answer" alias="Answer" />
		<typeAlias type="com.eunhasu.kickvillage.model.Card" alias="Card" />
		<typeAlias type="com.eunhasu.kickvillage.model.Email" alias="Email" />
	</typeAliases>
	
	<mappers>
		<mapper resource="query/mypage.xml"></mapper>
		<mapper resource="query/admin.xml"></mapper>
		<mapper resource="query/station.xml"></mapper>
		<mapper resource="query/reserve.xml"></mapper>
		<mapper resource="query/board.xml"></mapper>
		<mapper resource="query/card.xml"></mapper>
	</mappers>
	
</configuration>